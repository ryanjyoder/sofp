#!/usr/bin/env bash

## Get ENVs ###
export SIMULTANEOUS_DOWNLOAD="$(snapctl get simultaneous-download)"
export SIMULTANEOUS_PARSE="$(snapctl get simultaneous-parse)"

 
if [ -z "$COUCHDB_HOST" ]
then
    exit 1
fi

export PATH=$SNAP/bin:$PATH

### Start daemon ###
$SNAP/bin/downloader $SNAP_COMMON
