#!/usr/bin/env bash

## Get ENVs ###
export COUCHDB_HOST="$(snapctl get couchdb.host)"
export COUCHDB_USER="$(snapctl get couchdb.user)"
export COUCHDB_PASS="$(snapctl get couchdb.pass)"

if [ -z "$COUCHDB_HOST" ]
then
    exit 1
fi

export PATH=$SNAP/bin:$PATH

### Start daemon ###
$SNAP/bin/downloader $SNAP_COMMON